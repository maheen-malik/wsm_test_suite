      },
      {
        "Duration": 30000000000,
        "TargetRPS": 500,
        "Description": "Ramp up to 500 RPS"
      },
      {
        "Duration": 60000000000,
        "TargetRPS": 500,
        "Description": "Hold at 500 RPS"
      },
      {
        "Duration": 30000000000,
        "TargetRPS": 1000,
        "Description": "Ramp up to 1000 RPS"
      },
      {
        "Duration": 60000000000,
        "TargetRPS": 1000,
        "Description": "Hold at 1000 RPS"
      },
      {
        "Duration": 30000000000,
        "TargetRPS": 0,
        "Description": "Ramp down to 0"
      }
    ]
  }
}
